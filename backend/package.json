{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:full": "concurrently \"npm run start:be\" \"npm run start:fe\"",
    "seed:user": "node seeder/seederUser.js",
    "seed:event": "node seeder/seederEvent.js",
    "seed:event:dates": "node seeder/seedEventDates.js",
    "seed:registration": "node seeder/seederRegistration.js",
    "seed:attendance": "node seeder/seederAttendance.js",
    "seed:approval": "node seeder/seedApprovalRequest.js",
    "seed:channel": "node seeder/seederChannel.js",
    "seed:post": "node seeder/seederPost.js",
    "seed:comment": "node seeder/seederComment.js",
    "seed:reaction": "node seeder/seederReaction.js",
    "seed:user:destroy": "node seeder/destroyUser.js",
    "seed:event:destroy": "node seeder/destroyEvent.js",
    "seed:registration:destroy": "node seeder/destroyRegistration.js",
    "seed:attendance:destroy": "node seeder/destroyAttendance.js",
    "seed:all": "node seeder/seedAll.js",
    "seed:destroy:all": "npm run seed:user:destroy && npm run seed:event:destroy && npm run seed:registration:destroy && npm run seed:attendance:destroy",
    "seed:reset": "npm run seed:destroy:all && npm run seed:all"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^1.37.2",
    "colors": "^1.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "firebase-admin": "^13.5.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^7.0.9",
    "socket.io": "^4.8.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10"
  }
}
